@model QandAn.ViewModels.ImportViewModel
@{
    ViewData["Title"] = "Paste answer";
    
}
@Html.AntiForgeryToken()
<h2>Import answer</h2>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Import">
            <div class="text-danger"></div>
             <div class="form-group">
                <label asp-for="Url">Title: </label>
                <input asp-for="Url"/>
                <span asp-validation-for="Url"/>
            </div>
            <div class="form-group">
                <input type="submit" value="Import" class="btn btn-warning" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
